[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
CN = api.example.com
O  = Example Inc

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = api.example.com
DNS.2 = www.example.com
IP.1 = 192.168.1.1
IP.2 = 192.168.1.2
