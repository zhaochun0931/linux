[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[dn]
C = US
ST = State
L = City
O = MyCompany
OU = IT
CN = gemfire1  # Primary domain

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = gemfire1-locator.gemfire.svc.cluster.local
DNS.2 = '*.gemfire1-locator.gemfire.svc.cluster.local'
DNS.3 = '*.gemfire1-server.gemfire.svc.cluster.local'
IP.1 = 10.1.1.1
         
